http://lunxue.cc http://www.lunxue.cc https://lunxue.cc https://www.lunxue.cc {
        log /data/logs/caddy/cunxun-ui/www.lunxue.cc.log
        gzip
        proxy /api localhost:8005 {
                policy round_robin
        }
}

